{
  "name": "gauvendi-ise-v2",
  "version": "0.0.0",
  "scripts": {
    "analyze-bundle": "ng build --stats-json && npx webpack-bundle-analyzer ./dist/gauvendi-ise-v2/stats.json",
    "build-prod": "ng build --configuration production && npm install --save-dev gzipper && gzipper c --gzip ./dist --include css,js ./dist/",
    "build-qa": "ng build --configuration qa && npm install --save-dev gzipper && gzipper c --gzip ./dist --include css,js ./dist/",
    "build": "ng build && npm install --save-dev gzipper && gzipper c --gzip ./dist --include css,js ./dist/",
    "generate-graphql": "gql-gen --config ./src/app/core/graphql/config/codegen.yml",
    "start": "ng serve --port 4202",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "upload-translation": "node --no-deprecation src/scripts/translation/update-static-content-translation.js"
  },
  "private": true,
  "dependencies": {
    "@adyen/adyen-web": "^3.11.0",
    "@angular/animations": "^17.2.3",
    "@angular/cdk": "^17.2.3",
