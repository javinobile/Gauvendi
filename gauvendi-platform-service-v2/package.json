{
  "name": "gauvendi-platform-service-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:prod": "rimraf dist && nest build --path tsconfig.build-prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm -- migration:generate src/core/database/migrations/{{Name}} -d src/core/database/data-source.ts",
    "migration:run": "nest build && NODE_ENV=production npx typeorm migration:run -d dist/core/database/data-source.js",
    "migration:run-windows": "npm run build && cross-env NODE_ENV=production npx typeorm migration:run -d dist/core/database/data-source.js",
    "migration:railway": "NODE_ENV=production npx typeorm migration:run -d dist/core/database/data-source.js",
    "migration:revert": "npm run build && NODE_ENV=production npx typeorm migration:revert -d dist/core/database/data-source.js",
    "migration:revert-windows": "npm run build && cross-env NODE_ENV=production npx typeorm migration:revert -d dist/core/database/data-source.js",
    "migration:show": "npm run build && NODE_ENV=production npx typeorm migration:show -d dist/core/database/data-source.js"
  },
  "dependencies": {
    "@aws-sdk/client-quicksight": "3.948.0",
    "@aws-sdk/client-s3": "3.884.0",
    "@aws-sdk/s3-request-presigner": "3.884.0",
    "@keyv/redis": "5.1.5",
    "@nestjs/axios": "4.0.1",
    "@nestjs/bullmq": "11.0.3",
    "@nestjs/cache-manager": "3.1.0",
    "@nestjs/common": "11.1.11",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.11",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "11.1.11",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/platform-express": "11.1.11",
    "@nestjs/swagger": "11.2.3",
    "@nestjs/typeorm": "11.0.0",
    "@sendgrid/mail": "8.1.6",
    "amqp-connection-manager": "5.0.0",
    "amqplib": "0.10.9",
    "async-mutex": "0.5.0",
    "axios": "1.13.2",
    "bullmq": "5.58.5",
    "cache-manager": "7.2.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "decimal.js": "10.6.0",
    "exceljs": "4.4.0",
    "i18n-iso-countries": "7.14.0",
    "ioredis": "5.8.1",
    "lodash": "4.17.21",
    "mongoose": "8.18.2",
    "mysql2": "3.14.5",
    "openai": "5.22.1",
    "pg": "8.16.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.26",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.14",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.11",
    "@swc/cli": "0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "5.0.6",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.20",
    "@types/multer": "2.0.0",
    "@types/node": "22.10.7",
    "@types/supertest": "6.0.2",
    "cross-env": "7.0.3",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}